{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "Client",
      "image": "ghaxhi/fibonacci-client",
      "hostname": "client",
      "essential": false,
      "memory": 128
    }, 
    {
      "name": "Server",
      "image": "ghaxhi/fibonacci-server",
      "hostname": "api",
      "essential": false,
      "memory": 128
    }, 
    {
      "name": "Worker",
      "image": "ghaxhi/fibonacci-worker",
      "essential": false,
      "memory": 128
    }, 
    {
      "name": "Nginx",
      "image": "ghaxhi/fibonacci-nginx",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": ["Client", "Server"],
      "memory": 128
    }
  ]
}